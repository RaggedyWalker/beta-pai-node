{
    "configurations": [

    {
        "name": "ts-node",
        "type": "node",
        "request": "launch",
        "args": [
            "${relativeFile}"
        ],
        "runtimeArgs": [
            "-r",
            "ts-node/register"
        ],
        "cwd": "${workspaceRoot}",
        "protocol": "inspector",
        "internalConsoleOptions": "openOnSessionStart"
    },
    {
        "name": "start:dev",
        "type": "node",
        "request": "launch",
        "args": [
            "${relativeFile}"
        ],
        "runtimeArgs": [
            "-r",
            "ts-node/register"
        ],
        "cwd": "${workspaceRoot}/bin/www.ts",
        "protocol": "inspector",
        "internalConsoleOptions": "openOnSessionStart"
    }
    ]
}